var tda_dl = {
    "site_hierarchy": window.location.pathname.split('/'),
    "page_name": document.title,
    "user_type": "",
    'site_name': 'freshaccounts',
    "site_domain": window.location.hostname,
    "responsive_experience": "desktop experience",
    "site_environment": (function () {
        if (document.URL.match('q4')) {
            return "qa"
        } else {
            return "prod"
        }
    }()),
    "user_ip": '',
    "server_timestamp": Date.now(),
    "language": "en-us",
    "screen_orientation": window.screen.orientation.type,
    "market_day": "",
    "onsite_search_term": (function () {
        var searchTerm = "";
        //Enter Key Press 
        jQuery(document).keypress(function (e) {
            searchTerm = jQuery('#_ctrl0_ctl15_divModuleContainer div div').eq(0).context.activeElement.value
            if (e.which == 13) {
                return searchTerm
            }
        });
        //Search Button Click 
        jQuery('.SearchButton').eq(0).on('click', function () {
            searchTerm = jQuery('#_ctrl0_ctl15_divModuleContainer div div')[0].childNodes[1].value
            return searchTerm
        })
    }())
};
